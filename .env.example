# Isabl API Configuration
ISABL_API_URL=https://api.isabl.io/api/v1/
ISABL_API_TOKEN=your-token-here

# Optional: Local paths to app repositories (for search_apps tool)
ISABL_APPS_PATH=/path/to/isabl_apps
ISABL_SHAHLAB_APPS_PATH=/path/to/shahlab_apps

# Optional: SSL and timeout settings
ISABL_VERIFY_SSL=true
ISABL_TIMEOUT=30
ISABL_LOG_LEVEL=INFO
